{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tpl5",
      "camelcase": "Tpl5",
      "title": "Tpl5",
      "scope": "source.tpl5",
      "file-types": [
        "tpl5"
      ],
      "injection-regex": "^tpl5$",
      "class-name": "TreeSitterTpl5"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "thinkphp template",
    "authors": [
      {
        "name": "erasin",
        "email": "erasinoo@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-tpl5"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}